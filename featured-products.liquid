{{ 'featured-products-custom.css' | asset_url | stylesheet_tag }}

<section class="featured-products flex flex-col gap-6 py-6 px-9 md:py-20 md:px-32">

  <div class="top">
      <div class="heading flex flex-row justify-between items-center">
        <h2 class="font-bold md:font-black text-center md:text-left w-full md:w-auto">{{ section.settings.headline }}</h2>
        <a href="{{ section.settings.products-url }}" class="rounded-full py-8 px-4 flex flex-row gap-2 hidden md:flex">
          <p class="tertiary-button font-semibold text-sm text-center">{{ section.settings.products-url-text }}</p>
          <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M3.33334 10H16.6667M16.6667 10L11.6667 5M16.6667 10L11.6667 15" stroke="#5C553A" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>
        </a>
      </div>
  </div>

  <div class="slider flex flex-col gap-3 md:gap-12">
      <div id="products" class="products-wrapper grid grid-cols-2 gap-2.5 md:flex md:flex-row md:gap-6 md:overflow-x-scroll relative">
        
        <div class="product-card flex flex-col pb-2 md:pb-4 gap-3 md:gap-4">
          {% if section.settings.product-1 %}
            {% assign product = section.settings.product-1 %}
              <a href="{{ product.url }}">
                  <div class="product-image overflow-hidden relative">
                  <div class="product-images relative w-full h-full">
                      <img class="product-image-first product-image-first-show absolute inset-0 w-full h-full object-cover" src="{{ product.featured_image | img_url: '1024x1024' }}" alt="{{ product.featured_image.alt | escape }}"/>
                      <img class="product-image-second product-image-second-hide absolute inset-0 w-full h-full object-cover" src="{{ product.images[1] | img_url: '1024x1024' }}" alt="{{ product.images[1].alt | escape }}"/>
                  </div>
                  <div class="best-sellers-badge absolute top-1 left-1 md:top-2 md:left-2 rounded-full border py-1 px-2 bg-white border-black">
                      <p class="uppercase font-normal md:leading-3 text-center text-black">Best Sellers</p>
                  </div>
                  {% if product.compare_at_price > product.price %}
                    <div class="sale-badge absolute right-1 top-1 md:top-2 rounded-full border py-1 px-2 border-black">
                      <p class="uppercase font-normal md:leading-3 text-center text-white">Save {{ product.compare_at_price | minus: product.price | times: 100 | divided_by: product.compare_at_price | round | append: '%' }}</p>
                    </div>
                  {% endif %}
                  </div>
              </a>
              <div class="details flex flex-column gap-2 md:gap-5">
                <div class="px-1 md:px-3 flex flex-col gap-2">
                  <p class="product-title uppercase font-normal text-lg">{{ product.title }}</p>
                  <div class="flex flex-row gap-1">
                    <div class="flex flex-row gap-0.5 items-center">
